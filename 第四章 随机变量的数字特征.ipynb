   "source": [
    "from scipy.stats import binom\n",
    "\n",
    "rv = binom(10, 0.2)\n",
    "print '期望：',rv.mean()\n",
    "print '方差：',rv.var()\n",
    "print '一阶矩：',rv.moment(1)\n",
    "print '二阶矩：',rv.moment(2)\n",
    "print '三阶矩：',rv.moment(3)\n",
    "print '四阶矩：',rv.moment(4)\n",
    "print '(期望、方差、偏度、峰度)：',rv.stats(moments = 'mvsk')"
   ]
  }
